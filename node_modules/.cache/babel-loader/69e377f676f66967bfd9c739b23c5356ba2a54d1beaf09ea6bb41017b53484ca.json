{"ast":null,"code":"export default {\n  name: \"Search\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      goodsList: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 8,\n      // 每页显示的个数\n      total: 0,\n      name: this.$route.query.name\n    };\n  },\n  created() {\n    this.loadGoods(1);\n  },\n  methods: {\n    loadGoods(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/goods/search', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          userId: this.user.id,\n          name: this.name\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.goodsList = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.loadGoods(pageNum);\n    },\n    reset() {\n      this.name = null;\n      this.loadGoods(1);\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","goodsList","pageNum","pageSize","total","$route","query","created","loadGoods","methods","$request","get","params","userId","id","then","res","code","list","$message","error","msg","handleCurrentChange","reset"],"sources":["src/views/front/Search.vue"],"sourcesContent":["<template>\r\n  <div style=\"width: 70%; margin: 10px auto\">\r\n    <div style=\"margin-bottom: 10px\">\r\n      <el-input v-model=\"name\" size=\"medium\" style=\"width: 300px; margin-right: 10px\"></el-input>\r\n      <el-button type=\"primary\" @click=\"loadGoods(1)\">搜 索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\">重 置</el-button>\r\n    </div>\r\n    <div class=\"card\">\r\n      <el-row :gutter=\"15\">\r\n        <el-col :span=\"6\" v-for=\"item in goodsList\" :key=\"item.id\">\r\n          <div style=\"margin-bottom: 20px\" class=\"goods-item\" @click=\"$router.push('/front/goodsDetail?id=' + item.id)\">\r\n            <img :src=\"item.img\" alt=\"\" style=\"width: 100%; display: block; height: 260px; margin-bottom: 10px; border-radius: 5px\">\r\n            <div style=\"font-size: 16px; height: 40px; color: #555; margin-bottom: 10px\" class=\"line2 goods-name\">{{ item.name }}</div>\r\n            <div style=\"display: flex; align-items: baseline\">\r\n              <strong style=\"color: red; font-size: 24px\">￥{{ item.price }}</strong>\r\n              <span style=\"margin-left: 20px; color: #666\">{{ item.readCount }}人浏览</span>\r\n              <span style=\"margin-left: 20px; color: #666\">{{ item.likesCount }}人点赞</span>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <div style=\"margin: 15px auto\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Search\",\r\n  data() {\r\n    return {\r\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n      goodsList: [],  // 所有的数据\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 8,  // 每页显示的个数\r\n      total: 0,\r\n      name: this.$route.query.name\r\n    }\r\n  },\r\n  created() {\r\n    this.loadGoods(1)\r\n  },\r\n  methods: {\r\n    loadGoods(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/goods/search', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          userId: this.user.id,\r\n          name: this.name\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.goodsList = res.data?.list\r\n          this.total = res.data?.total\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.loadGoods(pageNum)\r\n    },\r\n    reset() {\r\n      this.name = null\r\n      this.loadGoods(1)\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"mappings":"AAuCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAV,IAAA,OAAAW,MAAA,CAAAC,KAAA,CAAAZ;IACA;EACA;EACAa,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAAN,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAQ,QAAA,CAAAC,GAAA;QACAC,MAAA;UACAV,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAU,MAAA,OAAAjB,IAAA,CAAAkB,EAAA;UACApB,IAAA,OAAAA;QACA;MACA,GAAAqB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAhB,SAAA,GAAAe,GAAA,CAAArB,IAAA,EAAAuB,IAAA;UACA,KAAAd,KAAA,GAAAY,GAAA,CAAArB,IAAA,EAAAS,KAAA;QACA;UACA,KAAAe,QAAA,CAAAC,KAAA,CAAAJ,GAAA,CAAAK,GAAA;QACA;MACA;IACA;IACAC,oBAAApB,OAAA;MACA,KAAAM,SAAA,CAAAN,OAAA;IACA;IACAqB,MAAA;MACA,KAAA7B,IAAA;MACA,KAAAc,SAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}