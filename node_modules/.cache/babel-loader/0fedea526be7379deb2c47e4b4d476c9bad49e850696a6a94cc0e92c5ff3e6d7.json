{"ast":null,"code":"export default {\n  data() {\n    return {\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 8,\n      // 每页显示的个数\n      total: 0,\n      category: '全部',\n      sort: '最新',\n      categoryList: [],\n      GoodsList: []\n    };\n  },\n  mounted() {\n    this.loadCategory();\n    this.loadGoods(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    loadCategory() {\n      this.$request.get('/category/selectAll').then(res => {\n        if (res.code == '200') {\n          this.categoryList = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    loadGoods(pageNum) {\n      // 分页查询\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/goods/selectFrontPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          category: this.category == '全部' ? null : this.category,\n          sort: this.sort\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.GoodsList = res.data?.list;\n          this.total = res.data?.total;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.loadGoods(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["data","pageNum","pageSize","total","category","sort","categoryList","GoodsList","mounted","loadCategory","loadGoods","methods","$request","get","then","res","code","$message","error","msg","params","list","handleCurrentChange"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<template>\r\n  <div style=\"min-height: 100vh;\">\r\n\r\n    <div class=\"bg\">\r\n      <div style=\"width: 600px;\">\r\n        <div style=\"text-align: center; color: #eeeeee; font-size: 30px; margin-bottom: 20px;\">好看、好用、好玩，都在这里</div>\r\n        <div style=\"display: flex;\">\r\n          <el-input size=\"medium\" prefix-icon=\"el-icon-search\" placeholder=\"请输入商品名称\"></el-input>\r\n          <el-button type=\"primary\" size=\"medium\" style=\"margin-left: 5px; background-color: #409EFF\"><i class=\"el-icon-search\"></i></el-button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div style=\"width: 70%; margin: 10px auto; background-color: #fff; padding: 20px;\">\r\n      <div style=\"margin-bottom: 20px;\">\r\n        <el-select v-model=\"category\" size=\"medium\" style=\"width: 200px\" @change=\"loadGoods(1)\">\r\n          <el-option label=\"全部\" value=\"全部\"></el-option>\r\n          <el-option v-for=\"item in categoryList\" :key=\"item.id\" :label=\"item.name\" :value=\"item.name\"></el-option>\r\n        </el-select>\r\n        <el-select v-model=\"sort\" size=\"medium\" style=\"width: 200px; margin-left: 20px;\" @change=\"loadGoods(1)\">\r\n          <el-option label=\"最新\" value=\"最新\"></el-option>\r\n          <el-option label=\"最热\" value=\"最热\"></el-option>\r\n        </el-select>\r\n      </div>\r\n      <div>\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"6\" v-for=\"item in GoodsList\" :key=\"item.id\">\r\n            <div style=\"margin-bottom: 15px;\">\r\n              <img :src=\"item.img\" alt=\"\" class=\"goods-img\" @click=\"$router.push('/front/goodsDetail?id=' + item.id)\">\r\n              <div class=\"goods-name line2\">{{ item.name }}</div>\r\n              <div style=\"display: flex; align-items: baseline\">\r\n                <strong style=\"color: red; font-size: 24px\">￥{{ item.price }}</strong>\r\n                <span style=\"margin-left: 20px; color: #666\">{{ item.readCount }} 人浏览</span>\r\n                <span style=\"margin-left: 20px; color: #666\">100 人点赞</span>\r\n              </div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      <div class=\"pagination\" style=\"margin-top: 15px;\">\r\n        <el-pagination\r\n            background\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pageNum\"\r\n            :page-sizes=\"[5, 10, 20]\"\r\n            :page-size=\"pageSize\"\r\n            layout=\"total, prev, pager, next\"\r\n            :total=\"total\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n      pageNum: 1,   // 当前的页码\r\n      pageSize: 8,  // 每页显示的个数\r\n      total: 0,\r\n      category: '全部',\r\n      sort: '最新',\r\n      categoryList: [],\r\n      GoodsList: []\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadCategory()\r\n    this.loadGoods(1)\r\n  },\r\n  // methods：本页面所有的点击事件或者其他函数定义区\r\n  methods: {\r\n    loadCategory(){\r\n      this.$request.get('/category/selectAll').then(res => {\r\n        if(res.code == '200'){\r\n          this.categoryList = res.data\r\n        }else{\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    loadGoods(pageNum) {  // 分页查询\r\n      if (pageNum) this.pageNum = pageNum\r\n      this.$request.get('/goods/selectFrontPage', {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          category: this.category == '全部' ? null : this.category,\r\n          sort: this.sort,\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.GoodsList = res.data?.list\r\n          this.total = res.data?.total\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.loadGoods(pageNum)\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.bg{\r\n  height: 400px;\r\n  background-image: url(\"@/assets/imgs/home.jpg\");\r\n  background-size: 100%;\r\n  background-position-y: center;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n.goods-img{\r\n  width: 100%; \r\n  height: 300px; \r\n  display: block; \r\n  margin-bottom: 10px; \r\n  border-radius: 5px;\r\n  cursor: pointer;\r\n  transition: all 0.2s;\r\n}\r\n.goods-img:hover{\r\n  scale: 1.03;\r\n}\r\n.goods-name{\r\n  font-size: 14px; \r\n  height: 39px;\r\n  color: #555; \r\n  margin-bottom: 10px;\r\n}\r\n.goods-name:hover{\r\n  color: #15c0eb !important;\r\n}\r\n</style>\r\n"],"mappings":"AA0DA;EAEAA,KAAA;IACA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;MACAC,QAAA;MACAC,IAAA;MACAC,YAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;IACA,KAAAC,SAAA;EACA;EACA;EACAC,OAAA;IACAF,aAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,wBAAAC,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAV,YAAA,GAAAS,GAAA,CAAAf,IAAA;QACA;UACA,KAAAiB,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAT,UAAAT,OAAA;MAAA;MACA,IAAAA,OAAA,OAAAA,OAAA,GAAAA,OAAA;MACA,KAAAW,QAAA,CAAAC,GAAA;QACAO,MAAA;UACAnB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAE,QAAA,OAAAA,QAAA,uBAAAA,QAAA;UACAC,IAAA,OAAAA;QACA;MACA,GAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,SAAA,GAAAQ,GAAA,CAAAf,IAAA,EAAAqB,IAAA;UACA,KAAAlB,KAAA,GAAAY,GAAA,CAAAf,IAAA,EAAAG,KAAA;QACA;UACA,KAAAc,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAG,oBAAArB,OAAA;MACA,KAAAS,SAAA,CAAAT,OAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}