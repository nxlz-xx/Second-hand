{"ast":null,"code":"export default {\n  data() {\n    return {\n      id: this.$route.query.id,\n      goods: {},\n      current: '商品详情'\n    };\n  },\n  mounted() {\n    this.loadGoods();\n  },\n  methods: {\n    loadGoods() {\n      this.$request.get('/goods/selectById/' + this.id).then(res => {\n        if (res.code == '200') {\n          this.goods = res.data;\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    changeItem(current) {\n      this.current = current;\n    },\n    likes(num) {\n      let data = {\n        goodsId: this.id,\n        module: 'goods'\n      };\n      this.$request.post('/likes/add', data).then(res => {\n        if (res.code == '200') {\n          if (num == 1) {\n            this.$message.success(\"点赞成功\");\n          } else {\n            this.$message.success(\"已取消点赞\");\n          }\n          this.loadGoods();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    collect(num) {\n      let data = {\n        goodsId: this.id,\n        module: 'goods'\n      };\n      this.$request.post('/collect/add', data).then(res => {\n        if (res.code == '200') {\n          if (num) {\n            this.$message.success(\"收藏成功\");\n          } else {\n            this.$message.success(\"已取消收藏\");\n          }\n          this.loadGoods();\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","id","$route","query","goods","current","mounted","loadGoods","methods","$request","get","then","res","code","$message","error","msg","changeItem","likes","num","goodsId","module","post","success","collect"],"sources":["src/views/front/GoodsDetail.vue"],"sourcesContent":["<template>\r\n  <div style=\"margin: 10px auto; padding: 10px 0; width: 50%;\">\r\n    <div style=\"display: flex; margin-bottom: 40px; gap: 20px;\">\r\n        <img :src=\"goods.img\" alt=\"\" style=\"width: 50%; height: 400px; display: block;\">\r\n        <div>\r\n            <div style=\"font-weight: bold; font-size: 18px; margin: 20px auto;\">{{ goods.name }}</div>\r\n            <div style=\"display: flex; font-size: 16px;\">\r\n                <div><span style=\"color: #666;\">浏览</span> {{ goods.readCount }}</div>\r\n                <div style=\"margin-left: 30px;\"><span style=\"color: #666;\">点赞</span> {{ goods.likesCount }}</div>\r\n                <div style=\"margin-left: 30px;\"><span style=\"color: #666;\">收藏</span> {{ goods.collectCount }}</div>\r\n            </div>\r\n            <div style=\"color: red; font-size: 24px; font-weight: bold; margin: 30px 0;\">￥ {{ goods.price }}</div>\r\n            <div style=\"margin-bottom: 20px; font-size: 15px;\"><span style=\"color: #666\">发货地：</span> {{ goods.address }}</div>\r\n            <div style=\"margin-bottom: 20px; font-size: 15px;\"><span style=\"color: #666\">卖家：</span> {{ goods.userName }}</div>\r\n            <div style=\"margin-bottom: 40px; font-size: 15px;\"><span style=\"color: #666\">发布日期：</span> {{ goods.date }}</div>\r\n            <div>\r\n                <el-button v-if=\"!goods.userLikes\" size=\"medium\" type=\"warning\" @click=\"likes(1)\">点赞</el-button>\r\n                <el-button v-if=\"goods.userLikes\" size=\"medium\" type=\"warning\" @click=\"likes(0)\">已点赞</el-button>\r\n                <el-button v-if=\"!goods.userCollect\" size=\"medium\" type=\"danger\" @click=\"collect(1)\">收藏</el-button>\r\n                <el-button v-if=\"goods.userCollect\" size=\"medium\" type=\"danger\" @click=\"collect(0)\">已收藏</el-button>\r\n                <el-button size=\"medium\" type=\"success\">立即购买</el-button>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <div>\r\n        <div style=\"display: flex; border-bottom: 1px solid orange; margin-bottom: 10px;\">\r\n            <div style=\"padding: 10px 20px; cursor: pointer;\" \r\n            :class=\"{'active' : current == '商品详情'}\" @click=\"changeItem('商品详情')\">商品详情\r\n            </div>\r\n            <div style=\"padding: 10px 20px; cursor: pointer;\" \r\n            :class=\"{'active' : current == '商品评论'}\" @click=\"changeItem('商品评论')\">商品评论\r\n            </div>\r\n        </div>\r\n        <div v-if=\"current == '商品详情'\">\r\n            <div v-html=\"goods.content\"></div>\r\n        </div>\r\n        <div v-else>\r\n            666\r\n        </div>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n\r\n  data() {\r\n    return {\r\n        id: this.$route.query.id,\r\n        goods: {},\r\n        current: '商品详情'\r\n    }\r\n  },\r\n  mounted() {  \r\n    this.loadGoods()\r\n  },\r\n  methods: {\r\n    loadGoods(){\r\n      this.$request.get('/goods/selectById/' + this.id).then(res => {\r\n        if(res.code == '200'){\r\n            this.goods = res.data\r\n        }else{\r\n            this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    changeItem(current){\r\n        this.current = current\r\n    },\r\n    likes(num){\r\n        let data = {\r\n            goodsId: this.id,\r\n            module: 'goods'\r\n        }\r\n        this.$request.post('/likes/add', data).then(res => {\r\n            if(res.code == '200'){\r\n                if(num == 1){\r\n                    this.$message.success(\"点赞成功\")\r\n                }else{\r\n                    this.$message.success(\"已取消点赞\")\r\n                }\r\n                this.loadGoods()\r\n            }else{\r\n                this.$message.error(res.msg)\r\n            }\r\n        })\r\n    },\r\n    collect(num){\r\n         let data = {\r\n            goodsId: this.id,\r\n            module: 'goods'\r\n        }\r\n        this.$request.post('/collect/add', data).then(res => {\r\n            if(res.code == '200'){\r\n                if(num){\r\n                    this.$message.success(\"收藏成功\")\r\n                }else{\r\n                    this.$message.success(\"已取消收藏\")\r\n                }\r\n                this.loadGoods()\r\n            }else{\r\n                this.$message.error(res.msg)\r\n            }\r\n        })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.active{\r\n    background-color: orangered;\r\n    color: #eee;\r\n}\r\n</style>"],"mappings":"AA8CA;EAEAA,KAAA;IACA;MACAC,EAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAF,EAAA;MACAG,KAAA;MACAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,SAAA;EACA;EACAC,OAAA;IACAD,UAAA;MACA,KAAAE,QAAA,CAAAC,GAAA,6BAAAT,EAAA,EAAAU,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAT,KAAA,GAAAQ,GAAA,CAAAZ,IAAA;QACA;UACA,KAAAc,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAC,WAAAZ,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;IACA;IACAa,MAAAC,GAAA;MACA,IAAAnB,IAAA;QACAoB,OAAA,OAAAnB,EAAA;QACAoB,MAAA;MACA;MACA,KAAAZ,QAAA,CAAAa,IAAA,eAAAtB,IAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,IAAAM,GAAA;YACA,KAAAL,QAAA,CAAAS,OAAA;UACA;YACA,KAAAT,QAAA,CAAAS,OAAA;UACA;UACA,KAAAhB,SAAA;QACA;UACA,KAAAO,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;IACAQ,QAAAL,GAAA;MACA,IAAAnB,IAAA;QACAoB,OAAA,OAAAnB,EAAA;QACAoB,MAAA;MACA;MACA,KAAAZ,QAAA,CAAAa,IAAA,iBAAAtB,IAAA,EAAAW,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,IAAAM,GAAA;YACA,KAAAL,QAAA,CAAAS,OAAA;UACA;YACA,KAAAT,QAAA,CAAAS,OAAA;UACA;UACA,KAAAhB,SAAA;QACA;UACA,KAAAO,QAAA,CAAAC,KAAA,CAAAH,GAAA,CAAAI,GAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}